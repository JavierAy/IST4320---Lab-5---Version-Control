def csusb(schoolName, schoolMascot):
  print(schoolName + " " + schoolMascot)

csusb("CSU, San Bernardino", "Coyote")

def college_experience(Great, Good, Bad):
  print("How did you do in college?\n" + Great) 
  
  college_experience(Great = "I had the time of my life, and made many friends", Good = "It was ok, C's get degrees!", Bad = "Never doing that again")
  
